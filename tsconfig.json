{
  "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitAny": true,
      "emitDeclarationOnly": false,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "allowJs": true,
      "jsx": "preserve",
      "lib": ["ES2020"]
  },
  "include": ["./service", "./client", "./website"]
}
